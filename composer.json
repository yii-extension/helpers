{
    "name": "yii-extension/widgets",
    "type": "library",
    "description": "Widgets for yii3",
    "keywords": [
        "yii3",
        "widgets"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "symfony/options-resolver": "^5.2",
        "yiisoft/arrays": "^1.0",
        "yiisoft/json": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.6",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/form": "^1.0@dev",
        "yiisoft/validator": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Widgets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Widgets\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
